{
  "name": "giovanny-baltazar-site",
  "version": "1.0.0",
  "private": true,
  "description": "giovanny-baltazar-site",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "check": "npm run lint && npm run format",
    "start": "run-s clean develop:*",
    "develop": "run-s clean develop:*",
    "develop:server": "gatsby develop",
    "deploy": "run-s clean deploy:*",
    "deploy:build": "gatsby build --prefix-paths",
    "deploy:publish": "gh-pages --no-history -d public",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mdx-js/react": "^3.0.1",
    "gatsby": "^5.13.3",
    "gatsby-plugin-emotion": "^8.13.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^5.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "npm-run-all2": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-photo-view": "^1.2.4"
  },
  "volta": {
    "node": "20.12.0",
    "pnpm": "8.15.6"
  },
  "devDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5"
  },
  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f"
}
